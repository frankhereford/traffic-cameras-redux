generator client {
  provider             = "prisma-client-py"
  interface            = "sync"
  recursive_type_depth = 5
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Image {
  id                  String      @id @default(cuid())


  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("images")
}